    "source_name": "Spotify",
    "entry_type": "streaming",
    "configs": {
      "input_directory": "personal-data/spotify",
      "filename_regex": "StreamingHistory",
      "filetype": "json",
      "dedup_key": ["source", "track", "endTime"]
    },
    "field_mappings": [
      {
        "src": "artistName",
        "target": "artist",
        "src_type": "str",
        "target_type": "str"
      },
      {
