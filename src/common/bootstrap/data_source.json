        "target_type": "number"
      },
      {
        "src": "endTime",
        "target": "endTime",
        "src_type": "str",
        "target_type": "datetime"
      },
      {
        "src": "msPlayed",
        "target": "playtimeMs",
        "src_type": "str",
        "target_type": "str"
      },
      {
        "target": "startTime",
        "target_type": "datetime",
        "functions": [
          "eval: (datetime.fromisoformat(\"$endTime\") - timedelta(milliseconds=$playtimeMs/1000)).isoformat()"
        ]
      }
    ]
  },
